---
layout: null
sitemap: false
---
[{% for post in site.posts %}
  {
    "title": {{ post.title | smartify | strip_html | jsonify }},
    "url" : {{ post.url | relative_url | jsonify }},
    "content" : {{ post.content | smartify | strip_html | strip_newlines | jsonify }},
    "categories" : [{% for category in post.categories %}
        {{ category | strip_html | jsonify }}
        {%- unless forloop.last -%},{%- endunless -%}
        {% endfor %}
    ],
    "tags":[{% for tag in post.tags %}
      {{ tag | strip_html | jsonify }}
      {%- unless forloop.last -%},{%- endunless -%}
      {% endfor %}
    ]
  }{%- unless forloop.last -%},{%- endunless -%}
  {% endfor %}
]